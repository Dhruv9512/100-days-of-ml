   "source": [
    "import seaborn as sns\n",
    "df = sns.load_dataset('tips')\n",
    "def change(name):\n",
    "    if name==\"Male\":\n",
    "        return 1;\n",
    "    else:\n",
    "        return 0;\n",
    "df[\"sex\"]=df[\"sex\"].map(change)\n",
    "df[\"sex\"].value_counts().plot(kind=\"pie\",autopct='%.2f')\n"
   ]
